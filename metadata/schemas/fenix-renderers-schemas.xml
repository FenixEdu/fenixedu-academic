		<slot name="codigoPostal" />
		<slot name="localidadeCodigoPostal" />
		<slot name="localidade" />
		<slot name="freguesiaMorada" />
		<slot name="concelhoMorada" />
		<slot name="distritoMorada" />
		<!-- Contacts -->
		<slot name="telefone" />
		<slot name="telemovel" />
		<slot name="email" />
		<slot name="enderecoWeb" />

		<slot name="isPassInKerberos" />
