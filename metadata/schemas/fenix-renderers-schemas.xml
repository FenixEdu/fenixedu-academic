			<property name="size" value="12" />
			<property name="maxLength" value="10" />
		</slot>
		<slot name="dataValidadeDocumentoIdentificacao">
			<property name="size" value="12" />
			<property name="maxLength" value="10" />
		</slot>
		<slot name="numContribuinte" />
