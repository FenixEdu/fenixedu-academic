<schema name="show.protocol.toList"	type="net.sourceforge.fenixedu.domain.protocols.Protocol">
	<slot name="protocolNumber" key="label.protocol.number"/>
	<slot name="signedDate" key="label.protocol.signedDate"/>
	<slot name="protocolAction.text" key="label.protocol.scientificAreas"/>
	<slot name="observations" key="label.protocol.observations"/>		
</schema>

<schema name="create.protocol" type="net.sourceforge.fenixedu.dataTransferObject.protocol.ProtocolFactory">
	<slot name="protocolNumber"	key="label.protocol.number"/>
	<slot name="beginDate" key="label.protocol.beginDate">
		<property name="size" value="10"/>
		<property name="format" value="dd/MM/yyyy"/>
	</slot>
	<slot name="endDate" key="label.protocol.endDate">
		<property name="size" value="10"/>
		<property name="format" value="dd/MM/yyyy"/>
	</slot>
	<slot name="signedDate" key="label.protocol.signedDate">
		<property name="size" value="10"/>
		<property name="format" value="dd/MM/yyyy"/>
	</slot>
	<slot name="actionTypes" key="label.protocol.actionTypes" layout="option-select">
		<property name="choiceType"	value="net.sourceforge.fenixedu.domain.protocols.util.ProtocolActionType" />
		<property name="eachLayout" value="this-does-not-exist" />
	</slot>
	<slot name="otherActionTypes" key="label.protocol.otherActionTypes" layout="longText">
		<property name="columns" value="40" />
		<property name="rows" value="3" />
	</slot>
	<slot name="observations" key="label.protocol.observations" layout="longText">
		<property name="columns" value="40" />
		<property name="rows" value="3" />
	</slot>	
</schema>

