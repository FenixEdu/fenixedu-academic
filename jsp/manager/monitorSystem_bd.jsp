<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%@ taglib uri="/WEB-INF/taglibs-datetime.tld" prefix="dt" %><h2><bean:message key="manager.monitor.system.title"/></h2><br /><html:link page="/monitorSystem.do?method=monitor">	<bean:message key="manager.monitor.system.refresh"/></html:link><br /><br /><br /><logic:present name="systemInfoApplicationServer">	<bean:message key="manager.monitor.system.applicationServer"/>	<br /><br />	<strong>Nota:</strong> As unidades de memória são bytes.	<table>		<tr>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.availableProcessors"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.freeMemory"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.totalMemory"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.maxMemory"/>			</td>		</tr>		<tr>			<td class="listClasses">							<bean:write name="systemInfoApplicationServer" property="availableProcessors"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoApplicationServer" property="freeMemory"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoApplicationServer" property="totalMemory"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoApplicationServer" property="maxMemory"/>			</td>		</tr>	</table>	<br />	<logic:iterate id="property" name="systemInfoApplicationServer" property="properties">		<bean:write name="property"/><br />	</logic:iterate></logic:present><logic:notPresent name="systemInfoApplicationServer">	<span class="error">		Error obtaining log information for application server.	</span></logic:notPresent><br /><br /><logic:present name="systemInfoWebContainer">	<bean:message key="manager.monitor.system.webContainer"/>	<br /><br />	<strong>Nota:</strong> As unidades de memória são bytes.	<table>		<tr>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.availableProcessors"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.freeMemory"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.totalMemory"/>			</td>			<td class="listClasses-header">				<bean:message key="manager.monitor.system.maxMemory"/>			</td>		</tr>		<tr>			<td class="listClasses">							<bean:write name="systemInfoWebContainer" property="availableProcessors"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoWebContainer" property="freeMemory"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoWebContainer" property="totalMemory"/>			</td>			<td class="listClasses">				<bean:write name="systemInfoWebContainer" property="maxMemory"/>			</td>		</tr>	</table>	<br />	<logic:iterate id="property" name="systemInfoWebContainer" property="properties">		<bean:write name="property"/><br />	</logic:iterate></logic:present><logic:notPresent name="systemInfoWebContainer">	<span class="error">		Error obtaining log information for web container.	</span></logic:notPresent>