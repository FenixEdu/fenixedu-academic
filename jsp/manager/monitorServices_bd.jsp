<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><h2><bean:message key="manager.monitor.services.title"/></h2><br /><logic:present name="loggingIsOn">		<logic:equal name="loggingIsOn" value="true">			<span class="error">				Service monitorring is activated.			</span>			<br /><br />			<html:link page="/monitorServices.do?method=deactivateMonotoring">				<bean:message key="manager.monitor.services.deactivate-monitorring"/>			</html:link>			<html:link page="/monitorServices.do?method=clearServiceLogs">				<bean:message key="manager.monitor.services.clear-service-logs"/>			</html:link>			<html:link page="/monitorServices.do?method=monitor">				<bean:message key="manager.monitor.services.refresh"/>			</html:link>			<br /><br />			<logic:present name="serviceLogs">				<strong>Nota:</strong> As unidades de tempo são milisegundos.				<table>						<tr>							<td class="listClasses-header">								<bean:message key="manager.monitor.services.footPrint"/>							</td>							<td class="listClasses-header">								<bean:message key="manager.monitor.services.averageExecutionTime"/>							</td>							<td class="listClasses-header">								<bean:message key="manager.monitor.services.numberTimesCalled"/>							</td>							<td class="listClasses-header">								<bean:message key="manager.monitor.services.totalExecutionTime"/>							</td>							<td class="listClasses-header">								<bean:message key="manager.monitor.services.lastExecutionTime"/>							</td>						</tr>					<logic:iterate id="serviceLog" name="serviceLogs">						<tr>							<td class="listClasses">								<bean:write name="serviceLog" property="value.serviceFootPrint"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="value.averageExecutionTime"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="value.numberCalls"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="value.totalExecutionTime"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="value.lastExecutionTime"/>							</td>						</tr>					</logic:iterate>				</table>			</logic:present>		</logic:equal>		<logic:equal name="loggingIsOn" value="false">			<span class="error">				Service monitorring is deactivated.			</span>			<br /><br />			<html:link page="/monitorServices.do?method=activateMonotoring">				<bean:message key="manager.monitor.services.activate-monitorring"/>			</html:link>		</logic:equal></logic:present><logic:notPresent name="loggingIsOn">	<span class="error">		Error obtaining log information.	</span></logic:notPresent>