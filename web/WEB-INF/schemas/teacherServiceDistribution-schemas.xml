    	<slot name="department.realName" key="label.teacherServiceDistribution.department"/>
    </schema>
    
    <schema name="createTeacherDistribution" type="net.sourceforge.fenixedu.presentationTier.Action.departmentMember.teacherServiceDistribution.TeacherServiceBean">
		<slot name="department.realName" read-only="true" key="label.teacherServiceDistribution.department"/>
		<slot name="executionPeriod" layout="menu-select" key="label.common.courseSemester">
    		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.ExecutionPeriodsForExecutionYear"/>
			<property name="format" value="${name}"/>
    	</slot>
		<slot name="name" key="label.teacher.person.nome"/>
    </schema>    

