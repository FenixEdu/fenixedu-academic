    <schema name="selectPeriod" type="net.sourceforge.fenixedu.presentationTier.Action.departmentMember.teacherServiceDistribution.TeacherServiceBean">
    	<slot name="executionYear" layout="menu-select" key="label.common.courseYear">
    		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.OpenExecutionYearsProvider"/>
			<property name="format" value="${year}"/>
    	</slot>
    	<slot name="executionPeriod" layout="menu-select" key="label.common.courseSemester">
    		<property name="providerClass" value="net.sourceforge.fenixedu.presentationTier.renderers.providers.ExecutionPeriodsForExecutionYear"/>
			<property name="format" value="${name}"/>
    	</slot>
    </schema>
    
    <schema name="presentTSDProcess" type="net.sourceforge.fenixedu.domain.teacherServiceDistribution.TSDProcess">
    	<slot name="name" key="label.teacher.person.nome"/>
