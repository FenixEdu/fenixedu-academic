<!-- ******************************************************************************************************************************* -->
<!-- Definitions for net.sourceforge.fenixedu.domain.ExecutionPeriod -->
<!-- ******************************************************************************************************************************* -->
 <class-descriptor
         class="net.sourceforge.fenixedu.domain.ExecutionPeriod"
		 proxy="dynamic"
         table="EXECUTION_PERIOD">
       <field-descriptor
          name="idInternal"
          column="ID_INTERNAL"
          jdbc-type="INTEGER"
          primarykey="true"
          autoincrement="true"/>
       <field-descriptor
          name="name"
          column="NAME"
          jdbc-type="VARCHAR"/>
        <field-descriptor
          name="keyExecutionYear"
          column="KEY_EXECUTION_YEAR"
          jdbc-type="INTEGER"/>
        <field-descriptor
          name="keyPreviousExecutionPeriod"
          column="KEY_PREVIOUS_EXECUTION_PERIOD"
          jdbc-type="INTEGER"/>
		<field-descriptor
          name="keyNextExecutionPeriod"
          column="KEY_NEXT_EXECUTION_PERIOD"
          jdbc-type="INTEGER"/>
        <field-descriptor
          name="state"
          column="STATE"
	      conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaPeriodState2SqlPeriodState"
          jdbc-type="VARCHAR"/>
        <field-descriptor
          name="semester"
          column="SEMESTER"
          jdbc-type="INTEGER"/>          
        <field-descriptor
          name="beginDate"
          column="BEGIN_DATE"
          jdbc-type="DATE"
          conversion="org.apache.ojb.broker.accesslayer.conversions.JavaDate2SqlDateFieldConversion"
          />
        <field-descriptor
          name="endDate"
          column="END_DATE"
          jdbc-type="DATE"
          conversion="org.apache.ojb.broker.accesslayer.conversions.JavaDate2SqlDateFieldConversion"
          />
       <field-descriptor
      	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="true"/>
	   <reference-descriptor
          name="executionYear"
          class-ref="net.sourceforge.fenixedu.domain.ExecutionYear">
          <foreignkey field-ref="keyExecutionYear"/>
       </reference-descriptor>      
       <reference-descriptor
         name="previousExecutionPeriod"
         class-ref="net.sourceforge.fenixedu.domain.ExecutionPeriod">
         <foreignkey field-ref="keyPreviousExecutionPeriod"/>
       </reference-descriptor> 
	   <reference-descriptor
         name="nextExecutionPeriod"
         class-ref="net.sourceforge.fenixedu.domain.ExecutionPeriod">
         <foreignkey field-ref="keyNextExecutionPeriod"/>
       </reference-descriptor>
    <collection-descriptor
			name="schoolClasses"
			element-class-ref="net.sourceforge.fenixedu.domain.SchoolClass"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
    <collection-descriptor
			name="associatedOtherTypeCreditLines"
			element-class-ref="net.sourceforge.fenixedu.domain.credits.OtherTypeCreditLine"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="associatedTeacherDegreeFinalProjectStudents"
			element-class-ref="net.sourceforge.fenixedu.domain.degree.finalProject.TeacherDegreeFinalProjectStudent"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="enrolments"
			element-class-ref="net.sourceforge.fenixedu.domain.Enrolment"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="associatedTeacherInstitutionWorkTime"
			element-class-ref="net.sourceforge.fenixedu.domain.teacher.workTime.TeacherInstitutionWorkTime"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="enrolmentPeriod"
			element-class-ref="net.sourceforge.fenixedu.domain.EnrolmentPeriod"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="oldInquiriesTeachersRes"
			element-class-ref="net.sourceforge.fenixedu.domain.inquiries.OldInquiriesTeachersRes"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="associatedExecutionCourses"
			element-class-ref="net.sourceforge.fenixedu.domain.ExecutionCourse"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="lessons"
			element-class-ref="net.sourceforge.fenixedu.domain.Lesson"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="oldInquiriesSummary"
			element-class-ref="net.sourceforge.fenixedu.domain.inquiries.OldInquiriesSummary"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
	<collection-descriptor
			name="oldInquiriesCoursesRes"
			element-class-ref="net.sourceforge.fenixedu.domain.inquiries.OldInquiriesCoursesRes"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="keyExecutionPeriod"/>
	</collection-descriptor>
</class-descriptor> 
