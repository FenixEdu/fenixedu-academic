<!-- ******************************** -->
<!-- Definitions for Dominio.Metadata -->
<!-- ******************************** -->
<class-descriptor class="Dominio.Metadata"
                  table="METADATA">
    <field-descriptor id="1"
        name="idInternal"
        column="ID_INTERNAL"
        jdbc-type="INTEGER"
        primarykey="true"
        autoincrement="true"
    />
    <field-descriptor id="2"
        name="keyExecutionCourse"
        column="KEY_EXECUTION_COURSE"
        jdbc-type="INTEGER"
	/>
    <field-descriptor id="3"
         name="metadataFile"
         column="METADATA_FILE"
    	 jdbc-type="LONGVARCHAR"
      />
    <field-descriptor id="4"
        name="visibility"
        column="VISIBILITY"
        jdbc-type="BIT"
	/>
      <field-descriptor
      	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="true"
      />
    <reference-descriptor
         name="executionCourse"
         class-ref="Dominio.ExecutionCourse"
         proxy="true"
    >
    <collection-descriptor
		name="visibleQuestions"
		element-class-ref="Dominio.Question"
		proxy="true">
		<inverse-foreignkey field-ref="keyMetadata"/>
		<query-customizer class="ServidorPersistente.customQueries.QuestionFilter">
	    </query-customizer>
	</collection-descriptor>
	<foreignkey field-id-ref="2"/>
	</reference-descriptor>
	<attribute
		attribute-name="cacheable"
     	attribute-value="false" />
</class-descriptor>

