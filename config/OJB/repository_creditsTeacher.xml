<!-- Definitions for Dominio.ShiftProfessorship -->
<class-descriptor  class="Dominio.ShiftProfessorship"  table="SHIFT_PROFESSORSHIP">

      <field-descriptor  name="idInternal"  column="ID_INTERNAL"  jdbc-type="INTEGER" primarykey="true" autoincrement="true"/>

      <field-descriptor name="keyProfessorship" column="KEY_PROFESSOR_SHIP" jdbc-type="INTEGER"/>

      <field-descriptor name="keyShift" column="KEY_SHIFT" jdbc-type="INTEGER"/>

      <field-descriptor name="percentage" column="PERCENTAGE" jdbc-type="DOUBLE"/>
      <field-descriptor
      	name="ackOptLock"
      	column="ACK_OPT_LOCK"
      	jdbc-type="INTEGER"
      	locking="true"
      />
      
      <reference-descriptor name="professorship" class-ref="Dominio.Professorship">
         <foreignkey field-ref="keyProfessorship"/>
      </reference-descriptor>
            
      <reference-descriptor name="shift" class-ref="Dominio.Turno">
         <foreignkey field-ref="keyShift"/>
      </reference-descriptor>    
</class-descriptor>


