<!-- ************************************************************************************************************ -->
<!-- Definitions for net.sourceforge.fenixedu.domain.Student -->
<!-- ************************************************************************************************************ -->
<class-descriptor class="net.sourceforge.fenixedu.domain.Student" table="STUDENT">
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	<field-descriptor
		name="number"
		column="NUMBER"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="personKey"
		column="KEY_PERSON"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="state"
		column="STATE"
		jdbc-type="INTEGER"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaStudentState2SqlStudentStateFieldConversion"         
	/>
	<field-descriptor
		name="degreeType"
		column="DEGREE_TYPE"
		jdbc-type="VARCHAR"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.JavaTipoCurso2SqlTipoCursoFieldConversion"         
	/>
	<field-descriptor
		name="studentKindKey"
		column="KEY_STUDENT_KIND"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="keyRegistrationYear"
		column="KEY_REGISTRATION_YEAR"
		jdbc-type="INTEGER"      	
	/>
	<field-descriptor
		name="payedTuition"
		column="PAYED_TUITION"
		jdbc-type="BIT"
	/>
	<field-descriptor
		name="specialSeason"
		column="SPECIAL_SEASON"
		jdbc-type="BIT"
	/>
	<field-descriptor
		name="enrollmentForbidden"
		column="ENROLLMENT_FORBIDDEN"
		jdbc-type="BIT"
	/>
	<field-descriptor
		name="entryPhase"
		column="ENTRY_PHASE"
		jdbc-type="INTEGER"
		conversion="net.sourceforge.fenixedu.persistenceTier.Conversores.EntryPhase2SqlEntryPhaseFieldConversion"         
	/>
	<field-descriptor
		name="ackOptLock"
		column="ACK_OPT_LOCK"
		jdbc-type="INTEGER"
		locking="true"
	/>
	<field-descriptor 
        name="entryGrade"
        column="ENTRY_GRADE"
        jdbc-type="DOUBLE"
	/>
	<field-descriptor 
        name="contigent"
        column="CONTIGENT"
        jdbc-type="VARCHAR"
	/>
	<field-descriptor 
        name="ingression"
        column="INGRESSION"
        jdbc-type="VARCHAR"
	/>
	<field-descriptor 
        name="istUniversity"
        column="IST_UNIVERSITY"
        jdbc-type="VARCHAR"
	/>
	<reference-descriptor
			name="person"
			class-ref="net.sourceforge.fenixedu.domain.Person"
			proxy="false"
			refresh="true">
		<foreignkey field-ref="personKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="studentKind"
			class-ref="net.sourceforge.fenixedu.domain.StudentKind"
			proxy="false">
		<foreignkey field-ref="studentKindKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="registrationYear"
			class-ref="net.sourceforge.fenixedu.domain.ExecutionYear"
			proxy="true">
		<foreignkey field-ref="keyRegistrationYear"/>
	</reference-descriptor>
	<collection-descriptor
			name="studentCurricularPlans"
			element-class-ref="net.sourceforge.fenixedu.domain.StudentCurricularPlan"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="studentKey"/>
	</collection-descriptor>
</class-descriptor>