<!-- ************************************************************************************************************ -->
<!-- Definitions for Dominio.Student -->
<!-- ************************************************************************************************************ -->
<class-descriptor class="Dominio.Student" table="STUDENT">
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	<field-descriptor
		name="number"
		column="NUMBER"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="personKey"
		column="KEY_PERSON"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="state"
		column="STATE"
		jdbc-type="INTEGER"
		conversion="ServidorPersistente.Conversores.JavaStudentState2SqlStudentStateFieldConversion"         
	/>
	<field-descriptor
		name="degreeType"
		column="DEGREE_TYPE"
		jdbc-type="INTEGER"
		conversion="ServidorPersistente.Conversores.JavaTipoCurso2SqlTipoCursoFieldConversion"         
	/>
	<field-descriptor id="6"
		name="studentKindKey"
		column="KEY_STUDENT_KIND"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="keyRegistrationYear"
		column="KEY_REGISTRATION_YEAR"
		jdbc-type="INTEGER"      	
	/>
	<field-descriptor
		name="payedTuition"
		column="PAYED_TUITION"
		jdbc-type="BIT"
	/>
	<field-descriptor
		name="enrollmentForbidden"
		column="ENROLLMENT_FORBIDDEN"
		jdbc-type="BIT"
	/>
	<field-descriptor
		name="entryPhase"
		column="ENTRY_PHASE"
		jdbc-type="INTEGER"
		conversion="ServidorPersistente.Conversores.EntryPhase2SqlEntryPhaseFieldConversion"         
	/>
	<field-descriptor
		name="keyRegistrationYear"
		column="KEY_REGISTRATION_YEAR"
		jdbc-type="INTEGER"      	
	/>
	<field-descriptor
		name="ackOptLock"
		column="ACK_OPT_LOCK"
		jdbc-type="INTEGER"
		locking="true"
	/>
	<reference-descriptor
			name="person"
			class-ref="Dominio.Pessoa"
			proxy="false"
			refresh="true">
		<foreignkey field-id-ref="3"/>
	</reference-descriptor>
	<reference-descriptor
			name="studentKind"
			class-ref="Dominio.StudentKind"
			proxy="false">
		<foreignkey field-ref="studentKindKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="registrationYear"
			class-ref="Dominio.ExecutionYear"
			proxy="true">
		<foreignkey field-ref="keyRegistrationYear"/>
	</reference-descriptor>
	<collection-descriptor
			name="studentCurricularPlans"
			element-class-ref="Dominio.StudentCurricularPlan"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="studentKey"/>
	</collection-descriptor>
</class-descriptor>