<!-- ************************************************************************************************************ -->
<!-- Definitions for Dominio.StudentCurricularPlan -->
<!-- ************************************************************************************************************ -->
<class-descriptor class="Dominio.StudentCurricularPlan" table="STUDENT_CURRICULAR_PLAN">
	<extent-class class-ref="Dominio.StudentCurricularPlanLEEC"/>

	<field-descriptor
		name="ojbConcreteClass"
		column="CLASS_NAME"
		jdbc-type="VARCHAR"
	/>
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
	/>
	<field-descriptor
		name="studentKey"
		column="KEY_STUDENT"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="degreeCurricularPlanKey"
		column="KEY_DEGREE_CURRICULAR_PLAN"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="currentState"
		column="CURRENT_STATE"
		jdbc-type="INTEGER"
		conversion="ServidorPersistente.Conversores.JavaStudentCurricularPlanState2SqlStudentCurricularPlanStateFieldConversion"
	/>
	<field-descriptor
		name="startDate"
		column="START_DATE"
		jdbc-type="DATE"
	/>
	<field-descriptor
		name="specialization"
		column="SPECIALIZATION"
		jdbc-type="INTEGER"
		conversion="ServidorPersistente.Conversores.JavaSpecialization2SqlSpecializationFieldConversion"
	/>
	<field-descriptor
		name="branchKey"
		column="KEY_SPECIALIZATION_BRANCH"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="givenCredits"
		column="GIVEN_CREDITS"
		jdbc-type="FLOAT"
	/>
	<field-descriptor
		name="completedCourses"
		column="COMPLETED_COURSES"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="enrolledCourses"
		column="ENROLLED_COURSES"
		jdbc-type="INTEGER"
	/>
	<field-descriptor
		name="classification"
		column="CLASSIFICATION"
		jdbc-type="FLOAT"
	/>
	<field-descriptor 
		name="observations"
		column="OBSERVATIONS"
		jdbc-type="LONGVARCHAR"
	/>
    <field-descriptor
		name="employeeKey"
		column="KEY_EMPLOYEE"
		jdbc-type="INTEGER"	
	/>
	<field-descriptor
		name="when"
		column="WHEN_ALTER"
		jdbc-type="TIMESTAMP"
		conversion="ServidorPersistente.Conversores.TimeStamp2DateFieldConversion"
	/>
	<field-descriptor
		name="ackOptLock"
		column="ACK_OPT_LOCK"
		jdbc-type="INTEGER"
		locking="true"
	/>
	<reference-descriptor
			name="student"
			class-ref="Dominio.Student"
			proxy="true">
		<foreignkey field-ref="studentKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="degreeCurricularPlan"
			class-ref="Dominio.DegreeCurricularPlan"
			proxy="true">
		<foreignkey field-ref="degreeCurricularPlanKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="branch"
			class-ref="Dominio.Branch"
			proxy="true">
		<foreignkey field-ref="branchKey"/>
	</reference-descriptor>
	<reference-descriptor
			name="employee"
			class-ref="Dominio.Employee"
			proxy="true">
		<foreignkey field-ref="employeeKey"/>
	</reference-descriptor>
	<collection-descriptor
			name="enrolments"
			element-class-ref="Dominio.Enrolment"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="studentCurricularPlanKey"/>
	</collection-descriptor>    
	<collection-descriptor
			name="notNeedToEnrollCurricularCourses"
			element-class-ref="Dominio.degree.enrollment.NotNeedToEnrollInCurricularCourse"
			refresh="true"
			proxy="true">
		<inverse-foreignkey field-ref="studentCurricularPlanKey"/>
	</collection-descriptor>
</class-descriptor>

