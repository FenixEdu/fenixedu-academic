<class-descriptor class="net.sourceforge.fenixedu.domain.RootDomainObject" table="ROOT_DOMAIN_OBJECT">
	<field-descriptor
		name="idInternal"
		column="ID_INTERNAL"
		jdbc-type="INTEGER"
		primarykey="true"
		autoincrement="true"
		/>

	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector" 
			name="roles" element-class-ref="net.sourceforge.fenixedu.domain.Role">
		<inverse-foreignkey field-ref="keyRootDomainObject"/>
	</collection-descriptor>
	
	<collection-descriptor proxy="true" auto-retrieve="false" collection-class="org.apache.ojb.broker.util.collections.ManageableVector" 
			name="persons" element-class-ref="net.sourceforge.fenixedu.domain.Person">
		<inverse-foreignkey field-ref="keyRootDomainObject"/>
	</collection-descriptor>		
</class-descriptor>
