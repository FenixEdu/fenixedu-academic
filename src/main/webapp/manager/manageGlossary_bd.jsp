<%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><html:xhtml/><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><h2><bean:message bundle="MANAGER_RESOURCES" key="title.manage.glossary"/></h2><br /><br /><html:form action="/manageGlossary" >	<html:hidden bundle="HTMLALT_RESOURCES" altKey="hidden.page" property="page" value="1"/>	<html:hidden bundle="HTMLALT_RESOURCES" altKey="hidden.method" property="method" value="createGlossaryEntry"/>	<table>		<tr>			<td>				<bean:message bundle="MANAGER_RESOURCES" key="property.glossary.term"/>			</td>			<td>				<html:text bundle="HTMLALT_RESOURCES" altKey="text.term" size="45" property="term"/>			</td>		</tr>		<tr>			<td>				<bean:message bundle="MANAGER_RESOURCES" key="property.glossary.definition"/>			</td>			<td>				<html:text bundle="HTMLALT_RESOURCES" altKey="text.definition" size="45" property="definition"/>			</td>		</tr>	</table>	<br/>	<html:submit bundle="HTMLALT_RESOURCES" altKey="submit.submit" styleClass="inputbutton">		<bean:message bundle="MANAGER_RESOURCES" key="button.create"/>	</html:submit>	<html:reset bundle="HTMLALT_RESOURCES" altKey="reset.reset"  styleClass="inputbutton">		<bean:message bundle="MANAGER_RESOURCES" key="label.clear"/>	</html:reset>			</html:form><br /><logic:present name="infoGlossaryEntries">	<table width="90%" cellspacing="0" cellpadding="0" style="border: 1px solid #333;">		<logic:iterate id="infoGlossaryEntry" name="infoGlossaryEntries">			<bean:define id="entryId" name="infoGlossaryEntry" property="externalId"/>			<tr>				<td colspan="3" style="background: #ccc; padding: 10px 0 0 10px;">					<strong><bean:write name="infoGlossaryEntry" property="term"/>:</strong>				</td>				<td colspan="3" style="background: #ccc; padding: 10px 0 0 10px;">					<html:link module="/manager" page="<%= "/manageGlossary.do?method=deleteGlossaryEntry&amp;entryId="						+ pageContext.findAttribute("entryId") %>">						<bean:message bundle="MANAGER_RESOURCES" key="link.delete"/>					</html:link>				</td>			</tr>			<tr>				<td colspan="3" style="background: #fff; padding: 10px 0 0 10px;">					<bean:write name="infoGlossaryEntry" property="definition"/>				</td>				<td colspan="3" style="background: #fff; padding: 10px 0 0 10px;">				</td>			</tr>		</logic:iterate>	</table></logic:present>