<%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><html:xhtml/><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><h2><bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.title"/></h2><br /><logic:present name="loggingIsOn">		<logic:equal name="loggingIsOn" value="true">			<span class="error"><!-- Error messages go here -->				Service monitorring is activated.			</span>			<br /><br />			<html:link module="/manager" page="/monitorServices.do?method=deactivateMonotoring">				<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.deactivate-monitorring"/>			</html:link>			<html:link module="/manager" page="/monitorServices.do?method=clearServiceLogs">				<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.clear-service-logs"/>			</html:link>			<html:link module="/manager" page="/monitorServices.do?method=monitor">				<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.refresh"/>			</html:link>			<br /><br />			<logic:present name="serviceLogs">				<strong>Nota:</strong> As unidades de tempo sï¿½o milisegundos.				<table>						<tr>							<th class="listClasses-header">								<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.footPrint"/>							</th>							<th class="listClasses-header">								<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.averageExecutionTime"/>							</th>							<th class="listClasses-header">								<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.numberTimesCalled"/>							</th>							<th class="listClasses-header">								<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.totalExecutionTime"/>							</th>							<th class="listClasses-header">								<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.lastExecutionTime"/>							</th>						</tr>					<logic:iterate id="serviceLog" name="serviceLogs">						<tr>							<td class="listClasses">								<bean:write name="serviceLog" property="serviceFootPrint"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="averageExecutionTime"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="numberCalls"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="totalExecutionTime"/>							</td>							<td class="listClasses">								<bean:write name="serviceLog" property="lastExecutionTime"/>							</td>						</tr>					</logic:iterate>				</table>			</logic:present>		</logic:equal>		<logic:equal name="loggingIsOn" value="false">			<span class="error"><!-- Error messages go here -->				Service monitorring is deactivated.			</span>			<br /><br />			<html:link module="/manager" page="/monitorServices.do?method=activateMonotoring">				<bean:message bundle="MANAGER_RESOURCES" key="manager.monitor.services.activate-monitorring"/>			</html:link>		</logic:equal></logic:present><logic:notPresent name="loggingIsOn">	<span class="error"><!-- Error messages go here -->		Error obtaining log information.	</span></logic:notPresent>