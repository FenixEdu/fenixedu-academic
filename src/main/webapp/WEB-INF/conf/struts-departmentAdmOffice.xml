<!DOCTYPE struts-config PUBLIC
      "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
      "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
      <!-- ========== Data Source Configuration =============================== -->
      <!-- ========== Form Bean Definitions ================================== -->
      <form-beans type="org.apache.struts.action.ActionFormBean">
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherSearchForm">
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String" name="executionPeriodId"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherExecutionCourseShiftProfessorshipForm">
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.Integer" name="executionCourseId"></form-property>
                  <form-property type="java.lang.Integer" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.String" name="professorshipID"></form-property>
                  <form-property type="java.util.HashMap" name="teacherPercentageMap"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="method"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="supportLessonForm">
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.Integer" name="executionCourseId"></form-property>
                  <form-property type="java.lang.Integer" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.Integer" name="externalId"></form-property>
                  <form-property type="java.lang.String" name="supportLessonID"></form-property>
                  <form-property type="java.lang.Integer" name="infoProfessorshipId"></form-property>
                  <form-property type="java.lang.String" name="professorshipID"></form-property>
                  <form-property type="java.lang.String" name="weekDay"></form-property>
                  <form-property type="java.lang.String" name="startTimeHour"></form-property>
                  <form-property type="java.lang.String" name="startTimeMinutes"></form-property>
                  <form-property type="java.lang.String" name="endTimeHour"></form-property>
                  <form-property type="java.lang.String" name="endTimeMinutes"></form-property>
                  <form-property type="java.lang.String" name="place"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherDegreeFinalProjectStudentForm">
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.Integer" name="externalId"></form-property>
                  <form-property type="java.lang.Integer" name="teacherAdviseServiceID"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.Integer" name="studentId"></form-property>
                  <form-property type="java.lang.String" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.String" name="studentNumber"></form-property>
                  <form-property initial="100" type="java.lang.String" name="percentage"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherInstitutionWorkingTimeForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.Integer" name="externalId"></form-property>
                  <form-property type="java.lang.Integer" name="institutionWorkTimeID"></form-property>
                  <form-property type="java.lang.Integer" name="teacherServiceID"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.String" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.String" name="weekDay"></form-property>
                  <form-property type="java.lang.String" name="startTimeHour"></form-property>
                  <form-property type="java.lang.String" name="startTimeMinutes"></form-property>
                  <form-property type="java.lang.String" name="endTimeHour"></form-property>
                  <form-property type="java.lang.String" name="endTimeMinutes"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherExecutionCourseForm">
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property initial="false" type="java.lang.Boolean" name="responsibleFor"></form-property>
                  <form-property type="java.lang.String" name="executionDegreeId"></form-property>
                  <form-property type="java.lang.String" name="executionYearId"></form-property>
                  <form-property type="java.lang.String" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.String" name="executionCourseId"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="teacherExecutionCourseResponsabilities">
                  <form-property type="java.lang.String" name="teacherName"></form-property>
                  <form-property type="java.lang.String" name="externalId"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
                  <form-property type="java.lang.String" name="executionYearId"></form-property>
                  <form-property type="java.lang.String[]" name="executionCourseResponsability"></form-property>
                  <form-property type="java.util.HashMap" name="hours"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="searchTeachersDepartmentForm">
                  <form-property type="java.lang.Integer" name="externalId"></form-property>
                  <form-property type="java.lang.Integer" name="executionPeriodId"></form-property>
            </form-bean>
            <!-- ==================== View Curricular Course Historic ================== -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="executionYearDegreeCurricularPlanForm">
                  <form-property type="java.lang.String" name="executionYearID"></form-property>
                  <form-property type="java.lang.String" name="degreeCurricularPlanID"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="finalDegreeWorkForm">
                  <form-property type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="executionDegreeOID"></form-property>
                  <form-property type="java.lang.String" name="startOfProposalPeriodDate"></form-property>
                  <form-property type="java.lang.String" name="startOfProposalPeriodHour"></form-property>
                  <form-property type="java.lang.String" name="endOfProposalPeriodDate"></form-property>
                  <form-property type="java.lang.String" name="endOfProposalPeriodHour"></form-property>
                  <form-property type="java.lang.String[]" name="selectedProposals"></form-property>
                  <form-property type="java.lang.String" name="selectedGroupProposal"></form-property>
                  <form-property type="java.lang.String[]" name="attributedByTeacher"></form-property>
                  <form-property type="java.lang.String[]" name="attributions"></form-property>
                  <form-property type="java.lang.String" name="otherExecutionDegreeID"></form-property>
                  <form-property type="java.lang.Boolean" name="attributionByTeachers"></form-property>
                  <form-property type="java.lang.String" name="startOfCandidacyPeriodDate"></form-property>
                  <form-property type="java.lang.String" name="startOfCandidacyPeriodHour"></form-property>
                  <form-property type="java.lang.String" name="endOfCandidacyPeriodDate"></form-property>
                  <form-property type="java.lang.String" name="endOfCandidacyPeriodHour"></form-property>
                  <form-property type="java.lang.Boolean" name="allowSimultaneousCoorientationAndCompanion"></form-property>
                  <form-property type="java.lang.String" name="minimumNumberOfCompletedCourses"></form-property>
                  <form-property type="java.lang.String" name="maximumCurricularYearToCountCompletedCourses"></form-property>
                  <form-property type="java.lang.String" name="minimumCompletedCurricularYear"></form-property>
                  <form-property type="java.lang.String" name="minimumNumberOfStudents"></form-property>
                  <form-property type="java.lang.String" name="maximumNumberOfStudents"></form-property>
                  <form-property type="java.lang.String" name="maximumNumberOfProposalCandidaciesPerGroup"></form-property>
                  <form-property type="java.lang.String" name="minimumCompletedCreditsFirstCycle"></form-property>
                  <form-property type="java.lang.String" name="minimumCompletedCreditsSecondCycle"></form-property>
            </form-bean>
            <!-- SUBMIT FINAL WORK PROPOSAL -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="finalDegreeWorkProposal">
                  <form-property type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="executionDegreeOID"></form-property>
                  <form-property type="java.lang.String" name="degree"></form-property>
                  <form-property type="java.lang.String" name="externalId"></form-property>
                  <form-property type="java.lang.String" name="title"></form-property>
                  <form-property type="java.lang.String" name="orientatorOID"></form-property>
                  <form-property type="java.lang.String" name="coorientatorOID"></form-property>
                  <form-property type="java.lang.String" name="responsibleCreditsPercentage"></form-property>
                  <form-property type="java.lang.String" name="coResponsibleCreditsPercentage"></form-property>
                  <form-property type="java.lang.String" name="framing"></form-property>
                  <form-property type="java.lang.String" name="objectives"></form-property>
                  <form-property type="java.lang.String" name="description"></form-property>
                  <form-property type="java.lang.String" name="requirements"></form-property>
                  <form-property type="java.lang.String" name="deliverable"></form-property>
                  <form-property type="java.lang.String" name="url"></form-property>
                  <form-property size="0" type="java.lang.String[]" name="branchList"></form-property>
                  <form-property type="java.lang.String" name="minimumNumberOfGroupElements"></form-property>
                  <form-property type="java.lang.String" name="maximumNumberOfGroupElements"></form-property>
                  <form-property type="java.lang.String" name="degreeType"></form-property>
                  <form-property type="java.lang.String" name="observations"></form-property>
                  <form-property type="java.lang.String" name="location"></form-property>
                  <form-property type="java.lang.String" name="companyName"></form-property>
                  <form-property type="java.lang.String" name="companyAdress"></form-property>
                  <form-property type="java.lang.String" name="companionName"></form-property>
                  <form-property type="java.lang.String" name="companionMail"></form-property>
                  <form-property type="java.lang.String" name="companionPhone"></form-property>
                  <form-property type="java.lang.String" name="responsableTeacherName"></form-property>
                  <form-property type="java.lang.String" name="responsableTeacherId"></form-property>
                  <form-property type="java.lang.String" name="coResponsableTeacherName"></form-property>
                  <form-property type="java.lang.String" name="coResponsableTeacherId"></form-property>
                  <form-property type="java.lang.String" name="alteredField"></form-property>
                  <form-property type="java.lang.String" name="status"></form-property>
                  <form-property type="java.lang.String" name="degreeCurricularPlanID"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="creditsNotesForm">
                  <form-property type="java.lang.String" name="method"></form-property>
                  <form-property type="java.lang.String" name="noteType"></form-property>
                  <form-property type="java.lang.String" name="managementFunctionNote"></form-property>
                  <form-property type="java.lang.String" name="serviceExemptionNote"></form-property>
                  <form-property type="java.lang.String" name="otherNote"></form-property>
                  <form-property type="java.lang.String" name="masterDegreeTeachingNote"></form-property>
                  <form-property type="java.lang.String" name="functionsAccumulationNote"></form-property>
                  <form-property type="java.lang.String" name="thesisNote"></form-property>
                  <form-property type="java.lang.String" name="executionPeriodId"></form-property>
                  <form-property type="java.lang.String" name="teacherId"></form-property>
            </form-bean>
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="studentCurricularPlanAndEnrollmentsSelectionForm">
                  <form-property type="java.lang.String" name="studentCPID"></form-property>
                  <form-property type="java.lang.String" name="select"></form-property>
                  <form-property type="java.lang.String" name="studentNumber"></form-property>
                  <form-property initial="0" type="java.lang.Integer" name="page"></form-property>
                  <form-property type="java.lang.String" name="degreeCurricularPlanID"></form-property>
                  <form-property type="java.lang.String" name="organizedBy"></form-property>
                  <form-property initial="false" type="java.lang.Boolean" name="detailed"></form-property>
                  <form-property type="java.lang.String" name="viewType"></form-property>
            </form-bean>
            <!-- Summaries Management -->
            <form-bean dynamic="true" type="org.apache.struts.validator.DynaValidatorForm" name="summariesManagementForm">
                  <form-property initial="" type="java.lang.String" name="teacher"></form-property>
                  <form-property initial="" type="java.lang.String" name="teacherName"></form-property>
                  <form-property initial="" type="java.lang.String" name="teacherId"></form-property>
            </form-bean>
      </form-beans>
      <!-- ========== Global Exception Definitions ============================== -->
      <global-exceptions>
      </global-exceptions>
      <!-- ========== Global Forward Definitions =============================== -->
      <global-forwards type="org.apache.struts.action.ActionForward"></global-forwards>
      <!-- ========== Action Mapping Definitions =============================== -->
      <action-mappings type="org.apache.struts.action.ActionMapping">
      </action-mappings>
      <!-- ========== Controller Configuration ================================ -->
      <controller processorClass="pt.ist.fenixWebFramework.renderers.plugin.SimpleRenderersRequestProcessor"></controller>
      <!-- ========== Message Resources Definitions ============================ -->
      <message-resources null="false" parameter="resources.DepartmentAdmOfficeResources"></message-resources>
      <message-resources null="false" key="DEPARTMENT_ADM_OFFICE_RESOURCES" parameter="resources.DepartmentAdmOfficeResources"></message-resources>
      <message-resources null="false" key="DEPARTMENT_MEMBER_RESOURCES" parameter="resources.DepartmentMemberResources"></message-resources>
      <message-resources null="false" key="SITE_RESOURCES" parameter="resources.SiteResources"></message-resources>
      <message-resources null="false" key="ENUMERATION_RESOURCES" parameter="resources.EnumerationResources"></message-resources>
      <message-resources null="false" key="GLOBAL_RESOURCES" parameter="resources.GlobalResources"></message-resources>
      <message-resources null="false" key="DEFAULT" parameter="resources.ApplicationResources"></message-resources>
      <message-resources null="false" key="APPLICATION_RESOURCES" parameter="resources.ApplicationResources"></message-resources>
      <message-resources null="false" key="BOLONHA_MANAGER_RESOURCES" parameter="resources.BolonhaManagerResources"></message-resources>
      <message-resources null="false" key="ACADEMIC_OFFICE_RESOURCES" parameter="resources.AcademicAdminOffice"></message-resources>
      <message-resources null="false" key="GROUP_NAME_RESOURCES" parameter="resources.GroupNameResources"></message-resources>
      <message-resources null="false" key="RESEARCHER_RESOURCES" parameter="resources.ResearcherResources"></message-resources>
      <message-resources null="false" key="PEDAGOGICAL_COUNCIL" parameter="resources.PedagogicalCouncilResources"></message-resources>
      <message-resources null="false" key="TITLES_RESOURCES" parameter="resources.TitlesResources"></message-resources>
      <!-- ========== Plug Ins Configuration ================================= -->
      <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
            <set-property value="/WEB-INF/validator-rules.xml,/WEB-INF/fenix-validator-rules.xml,/WEB-INF/conf/validation-departmentAdmOffice.xml" property="pathnames"></set-property>
      </plug-in>
      <plug-in className="pt.ist.fenixWebFramework.struts.plugin.StrutsAnnotationsPlugIn"></plug-in>
</struts-config>