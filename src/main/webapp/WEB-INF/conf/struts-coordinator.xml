      <!--
		========== Action Mapping Definitions ===============================
	-->
      <action-mappings type="org.apache.struts.action.ActionMapping">
            <!-- page fronting actions -->
            <action forward="/switchAction.do?prefix=&amp;page=/mainPortalPage.jsp" path="/home"></action>
            <action type="org.apache.struts.actions.SwitchAction" path="/switchAction"></action>
            <action type="net.sourceforge.fenixedu.presentationTier.Action.commons.VoidAction" parameter="method" path="/candidateSection">
                  <forward path="/candidate/indexCandidate.jsp" name="Success"></forward>
            </action>
            <action type="net.sourceforge.fenixedu.presentationTier.Action.commons.VoidAction" parameter="method" path="/studentSection">
                  <forward path="/student/indexStudent.jsp" name="Success"></forward>
            </action>
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.coordinator.ReadCoordinatedDegreesAction" path="/index">
                  <forward path="/chooseDegreePage.jsp" name="ChooseDegree"></forward>
                  <forward path="/chooseDegree.do" name="oneDegreeSucces"></forward>
            </action>
            <!-- Dispatch Action to Choose the Degree -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.coordinator.CoordinatedDegreeInfo" parameter="method" path="/chooseDegree">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.ExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.ExistingActionException"></exception>
                  <forward path="/indexCoordinator.jsp" name="Success"></forward>
            </action>
            <!-- Dispatch Action to operate on the candidates -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.coordinator.CandidateOperationDispatchAction" scope="request" parameter="method" validate="true" input="/candidate/indexCandidate.jsp" path="/candidateOperation">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.NonExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.NonExistingActionException"></exception>
                  <forward path="/candidate/selectCandidateFromList.jsp" name="ViewList"></forward>
                  <forward path="/candidate/visualizeCandidate.jsp" name="ActionReady"></forward>
            </action>
            <!-- Dispatch Action to get the student list of the degree -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.coordinator.StudentListDispatchAction" scope="request" parameter="method" validate="true" input="/student/indexStudent.jsp" path="/studentListByDegree">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.NonExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.NonExistingActionException"></exception>
                  <forward path="/candidate/selectCandidateFromList.jsp" name="ViewList"></forward>
                  <forward path="/candidate/visualizeCandidate.jsp" name="ActionReady"></forward>
                  <forward path="df.page.chooseCurricularCourse" name="ShowCourseList"></forward>
            </action>
            <!-- Action for the Coordinator to List the Students -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.coordinator.StudentListDispatchAction" scope="request" input="/student/indexStudent.jsp" parameter="method" path="/listStudentsForCoordinator">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.NonExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.NonExistingActionException"></exception>
                  <forward path="df.page.displayStudentListByDegree" name="PrepareSuccess"></forward>
            </action>
            <!--
			Action for the Coordinator to List the Students by CurricularCourse
		-->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.commons.ChooseCurricularCourseDispatchAction" scope="request" parameter="method" path="/listStudentsByCourse">
                  <exception handler="net.sourceforge.fenixedu.presentationTier.config.FenixErrorExceptionHandler" key="resources.Action.exceptions.NonExistingActionException" type="net.sourceforge.fenixedu.presentationTier.Action.exceptions.NonExistingActionException"></exception>
                  <forward path="df.page.displayStudentListByCourse" name="ChooseSuccess"></forward>
                  <forward path="df.page.displayStudentListByDegree" name="PrepareSuccess"></forward>
                  <forward path="/studentListByDegree.do?method=getCurricularCourses" name="NoStudents"></forward>
                  <forward path="/student/notAuthorized.jsp" name="NotAuthorized"></forward>
            </action>
            <!-- List Student Curriculum -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.commons.student.CurriculumDispatchAction" scope="request" parameter="method" path="/studentCurriculum">
                  <forward path="/student/displayStudentCurriculum.jsp" name="ShowStudentCurriculum"></forward>
                  <forward path="/student/notAuthorized.jsp" name="NotAuthorized"></forward>
            </action>
            <!--
			Action for the Coordinator to List the Students Gratuity Situation
		-->
            <!-- Prepare the Candidates Approval Process -->
            <!-- Display List To Select Candidates -->
            <action type="net.sourceforge.fenixedu.presentationTier.Action.masterDegree.commons.candidate.SelectCandidatesDispatchAction" scope="request" parameter="method" name="chooseCandidateSituationForm" attribute="chooseCandidateSituationForm" input="/candidate/displayCandidateListBySituation.jsp" path="/displayListToSelectCandidates">
                  <forward path="/candidate/displayCandidateListBySituation.jsp" name="PrepareSuccess"></forward>
                  <forward path="/displayListToSelectCandidates.do?method=prepareSelectCandidates" name="CancelConfirmation"></forward>
                  <forward path="/candidate/warning.jsp" name="RequestConfirmation"></forward>
                  <forward path="/candidate/displayCandidatesFinalList.jsp" name="FinalPresentation"></forward>
                  <forward path="/displayListToSelectCandidates.do?method=preparePresentation&amp;page=0" name="ChooseSuccess"></forward>
                  <forward path="/displayListToSelectCandidates.do?method=getSubstituteCandidates&amp;page=0" name="OrderCandidates"></forward>
                  <forward path="/displayListToSelectCandidates.do?method=prepareSelectCandidates&amp;page=0" name="Cancel"></forward>
                  <forward path="/candidate/displayChosenSelection.jsp" name="OrderCandidatesReady"></forward>
                  <forward path="/candidate/numerusClaususNotDefined.jsp" name="NumerusClaususNotDefined"></forward>
                  <forward redirect="true" path="/backErrorCandidateApprovalPage.do" name="BackError"></forward>
                  <forward path="/candidate/approvalDispatchTemplate.jsp" name="PrintReady"></forward>
            </action>
            <action type="net.sourceforge.fenixedu.presentationTier.Action.commons.VoidAction" path="/backErrorCandidateApprovalPage">
                  <forward path="/candidate/backErrorPage.jsp" name="Success"></forward>
