		========== Plug Ins Configuration =================================
	-->
      <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
            <set-property value="/WEB-INF/validator-rules.xml,/WEB-INF/fenix-validator-rules.xml,/WEB-INF/conf/validation-coordinator.xml" property="pathnames"></set-property>
      </plug-in>
      <plug-in className="pt.ist.fenixWebFramework.struts.tiles.FenixTilesPlugin">
            <set-property value="/WEB-INF/tiles/layouts/tiles-layout-definitions.xml,/WEB-INF/tiles/coordinator/tiles-coordinator-definitions.xml,/WEB-INF/tiles/coordinator/tiles-coordinator-pages-definitions.xml" property="definitions-config"></set-property>
            <set-property value="true" property="moduleAware"></set-property>
            <set-property value="true" property="definitions-parser-validate"></set-property>
            <set-property value="pt.ist.fenixWebFramework.struts.tiles.FenixDefinitionsFactory" property="factoryClassname"></set-property>
            <set-property value="definition.coordinator.two-column" property="defaultTileDefinition"></set-property>
      </plug-in>
      <plug-in className="pt.ist.fenixWebFramework.struts.plugin.StrutsAnnotationsPlugIn"></plug-in>
