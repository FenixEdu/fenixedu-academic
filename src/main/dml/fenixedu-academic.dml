	class curriculum.grade.GradeScale {
		String code;
		LocalizedString name;
		BigDecimal minimumApprovedGrade;
		BigDecimal maximumApprovedGrade;
		BigDecimal minimumReprovedGrade;
		BigDecimal maximumReprovedGrade;
		boolean active;
		boolean internalGradeScale;
		boolean defaultGradeScale;
	}

		relation GradeScaleDomainRoot {
			.pt.ist.fenixframework.DomainRoot playsRole domainRoot {
				multiplicity 1..1;
			}
		
			curriculum.grade.GradeScale playsRole gradeScales {
				multiplicity *;
			}
		}
	
	class curriculum.grade.GradeScaleEntry {
		GradeScaleEnum gradeScaleEnum;
		String value (REQUIRED);
		LocalizedString description (REQUIRED);
		/* if true this grade is an approval */
		boolean allowsApproval;
		int gradeOrder;
	}
	
		relation GradeScaleEntryBennu {
			.org.fenixedu.bennu.core.domain.Bennu playsRole rootDomainObject {
				multiplicity 1..1;
			}
		
			curriculum.grade.GradeScaleEntry playsRole gradeScaleEntries {
				multiplicity *;
			}
		}
		
		relation GradeScaleGradeScaleEntry {
			curriculum.grade.GradeScale playsRole gradeScale {
				multiplicity 0..1;
			}
			
			curriculum.grade.GradeScaleEntry playsRole gradeScaleEntries {
				multiplicity *;
			}
		}

